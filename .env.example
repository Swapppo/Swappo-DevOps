# Swappo Environment Variables
# Copy this file to .env and update the values

# Auth Service Configuration
SECRET_KEY=your-secret-key-change-in-production
REFRESH_SECRET_KEY=your-refresh-secret-key-change-in-production
ACCESS_TOKEN_EXPIRE_MINUTES=30
REFRESH_TOKEN_EXPIRE_DAYS=7
ENVIRONMENT=production

# Database Credentials (CHANGE IN PRODUCTION!)
AUTH_DB_USER=swappo
AUTH_DB_PASSWORD=swappo_password
AUTH_DB_NAME=swappo_auth

CATALOG_DB_USER=swappo_user
CATALOG_DB_PASSWORD=swappo_pass
CATALOG_DB_NAME=swappo_catalog

# PgAdmin Configuration
PGADMIN_EMAIL=admin@swappo.com
PGADMIN_PASSWORD=admin
